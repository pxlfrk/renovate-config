{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": ["nextcloud"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/nextcloud/server"
    },
    {
      "matchPackageNames": ["goauthentik/authentik"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/goauthentik/authentik"
    },
    {
      "matchPackageNames": ["grafana/grafana"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/grafana/grafana"
    }
  ]
}
