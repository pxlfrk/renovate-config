{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": ["nextcloud"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/nextcloud/server"
    },
    {
      "matchPackageNames": ["goauthentik/authentik"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/goauthentik/authentik"
    },
    {
      "matchPackageNames": ["ghcr.io/requarks/wiki"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/requarks/wiki"
    },
    {
      "matchPackageNames": ["portainer/portainer-ee"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/portainer/portainer/"
    },
    {
      "matchPackageNames": ["grafana/grafana"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/grafana/grafana"
    },
    {
      "matchPackageNames": ["duplicati/duplicati"],
      "matchDatasources": ["docker"],
      "sourceUrl": "https://github.com/duplicati/duplicati"
    },
  ]
}
